{
    "name": "react-init",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@mui/icons-material": "^5.14.3",
        "@mui/material": "^5.14.4",
        "@mui/styled-engine-sc": "^5.12.0",
        "@reduxjs/toolkit": "^1.9.5",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^13.0.0",
        "@testing-library/user-event": "^13.2.1",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.0.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-redux": "^8.1.2",
        "react-router-dom": "^6.14.2",
        "react-scripts": "5.0.1",
        "redux": "^4.2.1",
        "redux-thunk": "^2.4.2",
        "styled-components": "^6.0.7",
        "tailwind-merge": "^1.14.0",
        "tailwindcss-animate": "^1.0.6",
        "web-vitals": "^2.1.0"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "start-env": "bash -ac '. ./envs/.start.env; react-scripts start'",
        "build-env": "bash -ac '. ./envs/.build.env; react-scripts build'",
        "test-env": "bash -ac '. ./envs/.test.env; react-scripts test'",
        "twatch": "npx tailwindcss -i ./src/index.css -o ./dist/output.css --watch",
        "lint": "npx eslint \"src/*.tsx\"",
        "pretty": "npx prettier 'src/**/*.ts' --write"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@types/eslint": "^8.44.2",
        "@types/jest": "^29.5.3",
        "@types/node": "^20.4.9",
        "@types/react": "^18.2.19",
        "@types/react-dom": "^18.2.7",
        "@typescript-eslint/eslint-plugin": "^5.52.0",
        "@typescript-eslint/parser": "^6.3.0",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^9.0.0",
        "eslint-config-standard-with-typescript": "^37.0.0",
        "eslint-import-resolver-typescript": "^3.5.5",
        "eslint-plugin-import": "^2.28.0",
        "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-promise": "^6.0.0",
        "eslint-plugin-react": "^7.33.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "prettier": "^3.0.1",
        "tailwindcss": "^3.3.3",
        "typescript": "*"
    }
}
